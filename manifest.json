{
  "version": "0.0.1",
  "name": "Meet Face Hack Extension",
  "description": "Meet の映像をハックして顔に画像を表示する",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["loader.js"],
      "matches": ["https://meet.google.com/*"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": ["main.js", "lmMarkImg.js"]
}
